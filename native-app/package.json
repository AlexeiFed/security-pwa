{
    "name": "security-pwa-native",
    "version": "1.0.0",
    "description": "Native mobile app for Security PWA with NaviKit SDK",
    "main": "index.js",
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint .",
        "build:android": "cd android && ./gradlew assembleRelease",
        "build:ios": "cd ios && xcodebuild -workspace SecurityPWA.xcworkspace -scheme SecurityPWA -configuration Release -destination generic/platform=iOS -archivePath SecurityPWA.xcarchive archive"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-native": "0.72.0",
        "@yandex/navi-kit-react-native": "^1.0.0",
        "@react-navigation/native": "^6.1.0",
        "@react-navigation/stack": "^6.3.0",
        "@react-navigation/bottom-tabs": "^6.5.0",
        "react-native-screens": "^3.20.0",
        "react-native-safe-area-context": "^4.5.0",
        "react-native-gesture-handler": "^2.10.0",
        "react-native-reanimated": "^3.0.0",
        "react-native-vector-icons": "^9.2.0",
        "@react-native-async-storage/async-storage": "^1.18.0",
        "react-native-geolocation-service": "^5.3.1",
        "react-native-permissions": "^3.8.0",
        "react-native-maps": "^1.7.1",
        "react-native-firebase": "^18.0.0",
        "@react-native-firebase/app": "^18.0.0",
        "@react-native-firebase/auth": "^18.0.0",
        "@react-native-firebase/firestore": "^18.0.0",
        "@react-native-firebase/messaging": "^18.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native/eslint-config": "^0.72.0",
        "@react-native/metro-config": "^0.72.0",
        "@tsconfig/react-native": "^3.0.0",
        "@types/react": "^18.0.24",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.2.1",
        "eslint": "^8.19.0",
        "jest": "^29.2.1",
        "metro-react-native-babel-preset": "0.76.5",
        "prettier": "^2.4.1",
        "react-test-renderer": "18.2.0",
        "typescript": "4.8.4"
    },
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-org/security-pwa-native.git"
    },
    "keywords": [
        "react-native",
        "navikit",
        "yandex",
        "navigation",
        "security",
        "pwa"
    ],
    "author": "Security PWA Team",
    "license": "MIT"
}